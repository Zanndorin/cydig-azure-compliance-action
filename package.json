    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.27",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chai": "^4.4.1",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "mocha": "^10.3.0",
    "prettier": "^2.8.8",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "typescript": "^5.4.2"
