{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2022",
    "module": "nodenext",
    "rootDir": "./",
    "moduleResolution": "nodenext",
    "baseUrl": "./",
    "sourceMap": true,
    "outDir": "./dist",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "newLine": "lf",
    "strict":true
  },
  "exclude": ["./dist", "./node_modules", "./src"]
}